<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>

<VBox alignment="CENTER" prefHeight="400.0" prefWidth="660.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ub.edu.view.EscenaTramTrackPdPDetalls">

   <AnchorPane id="anchorpane" minHeight="0.0" minWidth="0.0" prefHeight="280.0" prefWidth="600.0">
       <children>
           <!-- Titulo -->
           <Text layoutX="130.0" layoutY="30.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Tram Track">
               <font><Font size="35.0" /></font>
           </Text>

           <!-- Scroll de los Tram-->
           <ScrollPane layoutX="30.0" layoutY="50.0" prefHeight="250.0" prefWidth="600.0">
               <content>
                   <AnchorPane fx:id="tramTrack_pane" minHeight="0.0" minWidth="0.0" prefHeight="220.0" prefWidth="400.0">
                       <children>
                           <Button fx:id="tramTrack_btn" layoutX="20.0" layoutY="25.0" mnemonicParsing="false" prefHeight="24.0" prefWidth="60.0" />
                           <Text fx:id="tramTrack_text1" layoutX="100.0" layoutY="38.0" />
                           <Text fx:id="tramTrack_text2" layoutX="100.0" layoutY="58.0" textAlignment="CENTER"/>
                       </children>
                   </AnchorPane>
               </content>
           </ScrollPane>
       </children>
   </AnchorPane>
</VBox>
